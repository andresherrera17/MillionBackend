{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Mongo": {
    "ConnectionString": "process.env.MONGO_CONNECTION_STRING",
    "Database": "process.env.MONGO_DATABASE_NAME"
  },
  "ConnectionStrings": {
    "AzureBlobStorage": "process.env.BLOB_STORAGE_CONNECTION_STRING"
  },
  "BlobContainerName": "uploads",
  "AllowedHosts": "*",
  "AdminSettings": {
    "Password": "process.env.PASSWORD_ADMIN"
  },
  "Jwt": {
    "Key": "process.env.JWT_KEY",
    "Issuer": "Million.PropertiesApi",
    "Audience": "Million.Frontend",
    "ExpireMinutes": 120
  }
}

